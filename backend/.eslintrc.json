{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": "off",
    "space-before-function-paren": "error",
    "no-extra-semi": "error",
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1
      }
    ]
  }
}
